{
  "name": "codeceptjs-expect",
  "version": "1.0.0",
  "main": "dist/index.js",
  "description": "Expect helper for codeceptjs",
  "scripts": {
    "lint": "rome check src",
    "lint:fix": "rome check src --apply-suggested",
    "prepare": "husky install",
    "code:format": "rome format src --write",
    "build": "npx tsc -d -p tsconfig.json"
  },
  "keywords": [
    "codeceptjs-expect",
    "expect"
  ],
  "author": "kobenguyent <kobenguyent@gmail.com>",
  "files": [
    "dist/*",
    "README.md"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "expect": "^28.1.0"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "rome": "^11.0.0",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kobenguyent/codeceptjs-expect.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kobenguyent/codeceptjs-expect/issues"
  },
  "homepage": "https://github.com/kobenguyent/codeceptjs-expect#readme",
  "lint-staged": {
    "src": "rome format src --write"
  }
}
